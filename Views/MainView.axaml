          <Image PointerPressed="Image_PointerPressed" Source="{SvgImage /Assets/Images/nbpLogo.svg}" IsVisible="{Binding SideMenuExpanded}" Height="30"></Image>
          <Image PointerPressed="Image_PointerPressed" Source="{SvgImage /Assets/Images/list.svg}" Width="30" HorizontalAlignment="Left" IsVisible="{Binding !SideMenuExpanded}" Margin="5,0,0,0"></Image>
