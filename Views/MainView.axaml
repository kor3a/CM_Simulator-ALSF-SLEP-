        <StackPanel Grid.Row="1">
          <Button x:Name="homePg" Foreground="Black" Background="LightGray" Classes.active="{Binding HomePageIsActive}" Width="{Binding SideMenuItemWidth}" HorizontalAlignment="Left" Margin="0,5,0,0" Command="{Binding GoToHomeCommand}" Height="30">
            <StackPanel Orientation="Horizontal">
              <Image Source="{SvgImage /Assets/Images/home.svg}" Width="20"></Image>
              <Label Content="Home" IsVisible="{Binding SideMenuExpanded}" FontSize="11.5" Margin="5,0"></Label>
            </StackPanel>
          </Button>
