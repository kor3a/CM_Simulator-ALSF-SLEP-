                <Label Content="ICC 1" IsVisible="{Binding SideMenuExpanded}" FontSize="11.5" Margin="5,0"></Label>
              </StackPanel>
              <StackPanel Grid.Column="1" IsVisible="{Binding SideMenuExpanded}">
                <Border BorderThickness="2" BorderBrush="{Binding Icc1BorderBrush}" Background="{Binding Icc1BorderBackground}">
                  <Grid Width="50" Height="15">
                    <TextBlock Text="{Binding Icc1SideMenu}" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="11"/>
                  </Grid>
                </Border>
              </StackPanel>
            </Grid>
          </Button>
